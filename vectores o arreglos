// vectores o arreglos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main()
{
	int n;
	int numeros[100];
	int suma = 0;
	cout<<"ingrese el numero de elementos: ";
	cin>>n;
	for (int i=0;i<n;i++){
		cout<<"Ingrese el numero de elementos["<<i<<"]: ";
		cin>>numeros[i];
	}


	for(int i=0;i<n;i++){
		suma += numeros[i];
	}

	cout<<"La suma de los elementos del vector es: "<<suma<<endl;

	getch();
	return 0;
}
