// factorial con for.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
int i, n, factorial;
cout<< "introduce el valor de n: ";
cin>> n;
factorial=1;
for(i=1; i<=n; i++)
  factorial=factorial*i;
cout<< "factorial"<<factorial;
getch();
}
